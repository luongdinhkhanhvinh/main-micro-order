{
  "name": "@order/user-preference",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "engines": {
    "node": "16.x || 18.x",
    "npm": ">=6.9.x"
  },
  "devEngines": {
    "node": "16.x || 18.x",
    "npm": ">=6.9.x"
  },
  "scripts": {
    "--------------------------------  RUN & BUILD ----": "----",
    "start": "nest start",
    "prestart": "npm run migration:run",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "build": "tsc -p tsconfig.build.json",
    "check-engines": "node node_modules/fbjs-scripts/node/check-dev-engines.js package.json",
    "prebuild": "npm run check-engines && npm run clean",
    "build:prod": "npm run build",
    "clean": "rimraf dist/src/",
    "-------------------------------- TEST ----": "----",
    "test": "npm run test:migration && npm run test:ci && npm run test:e2e",
    "test:unit": "jest",
    "test:migration": "dotenv -e env.test --  typeorm migration:run -t each",
    "test:ci": "NODE_ENV=CI jest --ci none --testPathPattern='src' --forceExit true --collectCoverage --maxWorkers 2 --detectOpenHandles",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config jest.config-e2e.js --runInBand --silent --detectOpenHandles",
    "-------------------------------- MIGRATION ----": "----",
    "typeorm": "ts-node ./node_modules/typeorm/cli.js",
    "typeorm:cli": "ts-node ./node_modules/typeorm/cli -f ./ormconfig.ts",
    "migration:generate": "typeorm migration:create -n",
    "migration:run": "typeorm migration:run -t each",
    "migration:revert": "typeorm migration:revert",
    "db:sync": "npm run typeorm:cli schema:sync"
  },
  "dependencies": {
    "@nanogiants/nestjs-swagger-api-exception-decorator": "^1.6.11",
    "@nestjs/common": "^10.3.8",
    "@nestjs/core": "^10.3.8",
    "@nestjs/devtools-integration": "^0.1.6",
    "@nestjs/event-emitter": "^2.0.4",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.3.8",
    "@nestjs/swagger": "^7.3.1",
    "@nestjs/terminus": "^10.2.3",
    "@nestjs/typeorm": "^10.0.2",
    "@order/auth": "file:../../packages/auth",
    "@order/config": "file:../../packages/config",
    "@order/database": "file:../../packages/database",
    "@order/logger": "file:../../packages/logger",
    "@order/types": "file:../../packages/types",
    "bcrypt": "^5.1.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "debug": "^4.3.4",
    "express": "^4.19.2",
    "express-basic-auth": "^1.2.1",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "module-alias": "^2.2.3",
    "moment": "^2.30.1",
    "parse-domain": "^8.0.2",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "pg": "^8.11.5",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "swagger-ui-express": "^5.0.0",
    "typeorm": "^0.3.12",
    "url-join-ts": "^1.0.5",
    "uuid": "^9.0.1",
    "uuidv4": "^6.2.13",
    "winston": "^3.13.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.3.2",
    "@nestjs/schematics": "^10.1.1",
    "@nestjs/testing": "^10.3.8",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.2",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "^20.12.12",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "concurrently": "^8.2.2",
    "copyfile": "^0.1.2",
    "dotenv": "^16.4.5",
    "dotenv-cli": "^7.4.2",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "fbjs-scripts": "^3.0.1",
    "jest": "^29.5.0",
    "mock-jwks": "^3.1.0",
    "nock": "^13.5.4",
    "nodemon": "^3.1.0",
    "prettier": "^3.0.0",
    "rimraf": "^5.0.7",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.4",
    "ts-jest": "^29.1.3",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.4.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
